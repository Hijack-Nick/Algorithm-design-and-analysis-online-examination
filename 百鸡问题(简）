#include <iostream>
using namespace std; 
int main() {
    for (int x = 0; x <= 100 / 5; x++) 
	{
        for (int y = 0; y <= 100 / 3; y++) 
		{
            int z = 100 - x - y;  // 计算小鸡数量
            if (5 * x + 3 * y + z / 3 == 100 && z % 3 == 0) 
			{
                cout << x << " " << y << " " << z << endl;
            }
        }
    }

    return 0;
}
